  "type": "module",
  "bin": {
    "db-restore": "./build/cli.js"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "main": "./build/index.js",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf build",
    "dev": "tsx watch src/cli.ts",

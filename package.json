    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^4.0.10",
    "globals": "^17.0.0",
    "rimraf": "^6.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=24.0.0"
  },
  "volta": {
    "node": "24.13.1",
    "pnpm": "10.28.0"
